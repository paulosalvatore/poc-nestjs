model Speaker {
  id         Int      @id @default(autoincrement())
  user       User     @unique @relation(fields: [userId], references: [id])
  userId     Int
  courses_speakers_campuses     CourseSpeakerCampus[]
  meta       Meta     @relation(fields: [metaId], references: [id])
  metaId     Int
}

